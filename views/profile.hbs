<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="UTF-8">
	<title>DiscGolf</title>
	<!-- bootstrap css -->
	<link type="text/css" rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css">
	<!-- font -->
	<link href='https://fonts.googleapis.com/css?family=Arya' rel='stylesheet' type='text/css'>
	<!-- custom css -->
	<link type="text/css" rel="stylesheet" href="main.css">
</head>
<body>
	{{>header}}
	<div class="container">
		<div class="row">
			<div class="col-md-12">
				<h1 class="text-center">fore</h1>
			</div> {{!-- end title class --}}
		</div>  {{!-- end title row class --}}
		<div class="row">
			<div class="coursesDiv col-md-6">
				<h2>Local Courses</h2>
				<script id="course-template" type="text/x-handlebars-template">
					\{{#each courses}}
					<div class="mainList course_\{{course_id}}">
						<p class="CourseName">Course Name: \{{course_name}}</p>
						<p class="courseLocation">Address: \{{street}} \{{city}} \{{state_province}}, \{{postal_code}}</p>
						<button class="moreInfo">Course Details</button>
						<div class="courseDetailsDiv">
							<p class="courseDescription">Description: \{{course_description}}</p>
							<p class="courseFees">Fees: \{{fees}}</p>
							<p class="courseHoles">Holes: \{{holes}}</p>
							<p class="coursePrivate">Private: \{{private}}</p>
							<p class="externalLink1Url">Course Website: \{{external_link_1_url}}</p>
						</div>
					</div>
					\{{/each}}
				</script>
			</div>  {{!-- end of courses div --}}
			<div class="eventsDiv col-md-6">
				<h2>Local Events</h2>
				<script id="events-template" type="text/x-handlebars-template">
					\{{#each events}}
					<p>Course Name: \{{tournament_name}}</p>
				\{{/each}}
				</script>
			</div> {{!-- end of events div --}}
		</div> {{!-- end of main row --}}
	</div>  {{!-- end container --}}



	<!-- jquery -->
	<script type="text/javascript" src="https://code.jquery.com/jquery-1.11.3.min.js"></script>
	<!-- handlebars js -->
	<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/handlebars.js/4.0.3/handlebars.min.js"></script>
	<!-- custom js -->
	<script type="text/javascript" src="profile.js"></script>
</body>
</html>